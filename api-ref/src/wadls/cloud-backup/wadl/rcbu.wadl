<!--*******************************-->
<!--         Wake up agents         -->
<!--*******************************-->                           
      <method name="POST" id="postWakeUpAgents">
         <wadl:doc title="Wake up agents">
            <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Wakes up the agent before you perform tasks. </para>
            <para xmlns="http://docbook.org/ns/docbook">This operation sends a message to an agent to wakeup. You should wait 10 to 20 seconds after using this operation before starting a backup or restore.</para>
         </wadl:doc>
         <response status="200">
            <doc title="OK" xml:lang="EN">The request succeeded.</doc>
            <representation mediaType="application/json"/>
         </response>            
         <response status="400">
            <doc title="Bad Request" xml:lang="EN">There were one or more errors in the request.</doc>
            <representation mediaType="application/json"/>              
         </response>
         <response status="401">
            <doc title="Unauthorized" xml:lang="EN">The supplied token was not authorized to access the resources. Either it is expired or invalid.</doc>
            <representation mediaType="application/json"/>
         </response>
         <response status="403">
            <doc title="Forbidden" xml:lang="EN">Access to the requested resource was denied.</doc>
            <representation mediaType="application/json"/>
