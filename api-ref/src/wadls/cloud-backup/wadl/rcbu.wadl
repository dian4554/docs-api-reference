<!-- Migrate vault agent  -->
<!--**********************-->          
            <method name="PUT" id="migrateVault">
               <wadl:doc title="Migrate vault">
                  <para xmlns="http://docbook.org/ns/docbook" role="shortdesc">Migrates a backup vault from one machine agent to another.</para>
                  <para xmlns="http://docbook.org/ns/docbook">To use the migrate vault operation, the two agents should be under the same user and their backups should not be encrypted. The destination machine agent should not have any backups already configured and running.</para>
               </wadl:doc>
               <response status="200">
                  <doc title="OK" xml:lang="EN">The request succeeded.</doc>
                  <representation mediaType="application/json"/>
               </response>            
               <response status="403">
                  <doc title="Forbidden" xml:lang="EN">Access to the requested resource was denied.</doc>
                  <representation mediaType="application/json"/>
               </response>
               <response status="500">
                  <doc title="Instance Fault" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
               <response status="503">
                  <doc title="Service Unavailable" xml:lang="EN">This is a generic server error. The message contains the reason for the error. This error could wrap several error messages.</doc>
                  <representation mediaType="application/json"/>              
               </response>
            </method>
